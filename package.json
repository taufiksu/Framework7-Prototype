{
	"name": "framework7-prototype",
	"displayName": "Framework7 Prototype",
	"description": "Framework7 command to make application prototype",
	"version": "0.0.1",
	"icon": "framework7.png",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "ETTER",
	"author": {
		"name": "T Sulaeman P"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "f7.BlankPageSingle",
				"title": "Framework7: New Project Single Page (Windows)"
			},
			{
				"command": "f7.BlankPageSingleLinux",
				"title": "Framework7: New Project Single Page (Linux/Mac)"
			},
			{
				"command": "f7.Preview",
				"title": "Framework7: Open Preview in Panel"
			},
			{
				"command": "f7.PreviewBrowser",
				"title": "Framework7: Open Preview in Browser"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/taufiksu/framework7-prototype.git"
	}
}